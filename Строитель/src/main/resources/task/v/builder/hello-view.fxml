<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="388.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="task.v.builder.HelloController">
    <children>
      <VBox fx:id="vb" minHeight="300.0" minWidth="300.0" prefHeight="300.0" prefWidth="300.0">
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
         <children>
            <Pane prefHeight="200.0" prefWidth="400.0" />
         </children>
      </VBox>
      <HBox alignment="TOP_CENTER" prefHeight="47.0" prefWidth="410.0">
         <children>
              <Button fx:id="sadBtn" mnemonicParsing="false" onAction="#OnButtonSad" prefHeight="35.0" prefWidth="75.0" text="Грустный">
               <HBox.margin>
                  <Insets right="15.0" />
               </HBox.margin></Button>
              <Button fx:id="neutralBtn" mnemonicParsing="false" onAction="#OnButtonHappy" prefHeight="35.0" prefWidth="79.0" text="Весёлый">
               <HBox.margin>
                  <Insets right="15.0" />
               </HBox.margin>
            </Button>
              <Button fx:id="angryBtn" mnemonicParsing="false" onAction="#OnButtonAngry" prefHeight="35.0" prefWidth="75.0" text="Злой">
               <HBox.margin>
                  <Insets right="15.0" />
               </HBox.margin></Button>
              <Button fx:id="clearBtn" mnemonicParsing="false" onAction="#OnButtonClear" prefHeight="35.0" prefWidth="81.0" text="Очистить">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
    </children>
</VBox>
